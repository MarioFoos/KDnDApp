<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <size android:width="@dimen/butWealthH" android:height="@dimen/butWealthH"/>
            <solid android:color="@color/buttonDisableBg"/>
            <stroke android:color="@color/buttonDisableStroke" android:width="@dimen/butStroke" />
            <corners android:radius="@dimen/butCorner" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <size android:width="@dimen/butWealthH" android:height="@dimen/butWealthH"/>
            <solid android:color="@color/buttonNormalBg"/>
            <stroke android:color="@color/buttonNormalStroke" android:width="@dimen/butStroke" />
            <corners android:radius="@dimen/butCorner" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <size android:width="@dimen/butWealthH" android:height="@dimen/butWealthH"/>
            <solid android:color="@color/buttonNormalBg"/>
            <stroke android:color="@color/buttonNormalStroke" android:width="@dimen/butStroke" />
            <corners android:radius="@dimen/butCorner" />
        </shape>
    </item>
</selector>
